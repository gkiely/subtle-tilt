{
  "name": "Starter",
  "version": "1.0.0",
  "description": "Starter for front end projects",
  "main": "index.js",
  "scripts": {
    "build:dev": "gulp dev",
    "build:prod": "cross-env NODE_ENV=production gulp prod",
    "clean": "rimraf ./dist",
    "dll:dev": "webpack --config config/webpack.dev.dll.js",
    "dll:prod": "webpack --config config/webpack.prod.dll.js",
    "libsass": "npm rebuild node-sass",
    "setup": "yarn || (npm i yarn -g && yarn)",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "server:dev": "gulp",
    "server:prod": "",
    "start": "npm run setup && npm-run-all clean --parallel test dll:dev libsass build:dev --sequential server:dev",
    "production": "npm run setup && npm-run-all clean --parallel test dll:prod build:prod"
  },
  "devDependencies": {
    "babel-core": "6.9.1",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "^6.23.0",
    "class-to-classname": "^1.0.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-react": "5.1.1",
    "get-my-ip": "0.0.1",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.0",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "2.6.0",
