    "gulp-file-include": "^0.13.7",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "2.0.0",
    "gulp-livereload": "3.8.1",
    "gulp-newer": "1.1.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-noop": "^1.0.0",
    "gulp-processhtml": "^1.1.0",
    "gulp-sass": "2.1.1",
    "gulp-sass-glob": "1.0.6",
    "gulp-shell": "0.5.2",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "1.5.1",
    "gulp-webserver": "0.9.1",
